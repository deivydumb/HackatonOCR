{
  "name": "bbog-dig-dt-nodejs-mngr-template",
  "version": "1.0.7",
  "description": "Template Node.JS for managers with Express JS",
  "private": true,
  "main": "build/bin/www.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && export DEBUG=bdb:* && node ./build/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test:file": "export $(cat test/.env-test | xargs) && mocha --timeout 1000 --require ts-node/register",
    "test:only": "npm run test:file -- 'test/**/*.spec.ts'",
    "test": "export $(cat test/.env-test | xargs) && npm run test:only",
    "coverage": "export $(cat test/.env-test | xargs) && nyc _mocha --timeout 1000 --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:web": "npm run coverage && serve coverage/",
    "sonar": "gulp sonar",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "clean": "rm -rf .nyc_output coverage build .scannerwork performance/*.clinic-doctor*"
  },
  "keywords": [
    "typescript",
    "express",
    "js",
    "node-js"
  ],
  "author": "Steven Rodriguez",
  "license": "ISC",
  "dependencies": {
    "@npm-bbta/bbog-dig-dt-tslog-lib": "^3.7.0",
    "@npm-bbta/bbog-dig-evo-express-common-lib": "1.0.18",
    "aws-sdk": "^2.1451.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.17.3",
    "express-openapi-validate": "^0.6.1",
    "install": "^0.13.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.2.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.21",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^20.5.9",
    "@types/sinon": "^10.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1",
    "sinon": "^13.0.2",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
